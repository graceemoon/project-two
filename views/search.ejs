<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Search</title>
	<link rel="stylesheet" href="style.css">
	<link href="https://fonts.googleapis.com/css?family=Play:400,700|Press+Start+2P" rel="stylesheet">
</head>
<body>
	<h1>Search Pokemonsss</h1>

	<div class="pokemonSearch">
	  <form action="/search" method="get">
	    <input type="text" name="name" placeholder="Pokemon">
	    <input class="searchSubmit" type="submit" value="Search">
	  </form>
	</div>

  <div id="search-results-container">
    <h2>Search Results:</h2>
    <% pokes.forEach((pokemon) => { %>
      <div class="search-results">
        <p><%= pokemon.forms[0].name %></p>
        <p><%= pokemon.forms[0].url %></p>
      </div>
    <% }) %>
  </div>




</body>
</html>


