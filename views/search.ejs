<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Search</title>
	<link rel="stylesheet" href="style.css">
</head>
<body>
	<h1>Search Pokemonsss</h1>

	<div class="pokemonSearch">
	  <form action="search" method="POST">
	    <input type="text" name="search" placeholder="Pokemon">
	    <input type="submit" value="Search">
	  </form>
	</div>

  <div id="search-results-container">
    <h2>Search Results:</h2>
    <% pokes.forEach((pokemon) => { %>
      <div class="search-results">
        <p><%= pokemon.name %></p>
        <p><%= pokemon.url %></p>
      </div>
    <% }) %>
  </div>


  <div id="search-results-container">
    <h2>Search Params:</h2>
    <% poke.forEach((x) => { %>
      <div class="search-results">
        <p><%= x.name %></p>
        <p><%= x.sprite %></p>
      </div>
    <% }) %>
  </div>
  


</body>
</html>


